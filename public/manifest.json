{
  "short_name": "React App",
  "name": "React Extension",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
        "content.js"
    ],
    "run_at": "document_end"
}],
  "version": "1.0"
}
